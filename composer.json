{
    "name": "sjorsvanleeuwen/route-resource-extension",
    "description": "Resource extension to allow softDeletes",
    "license": "MIT",
    "authors": [
        {
            "name": "Sjors van Leeuwen",
            "email": "sjors@sjorsvanleeuwen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/routing": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SjorsvanLeeuwen\\RouteResourceExtension\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SjorsvanLeeuwen\\RouteResourceExtension\\ServiceProvider"
            ]
        }
    }
}
